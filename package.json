  "bugs": {
    "url": "https://github.com/abraham/twitter-d/issues"
  },
  "homepage": "https://github.com/abraham/twitter-d",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "pkg-ok": "^3.0.0",
    "typescript": "^4.0.0"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
