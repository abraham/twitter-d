{
  "name": "twitter-d",
  "version": "0.6.0",
  "description": "TypeScript definitions for Twitter API objects",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "build": "tsc",
